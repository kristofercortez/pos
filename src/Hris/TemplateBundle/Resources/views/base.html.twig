<!-- END PAGE LEVEL SCRIPTS -->
<script>
jQuery(document).ready(function() {
    $(".select2-search__field").attr("placeholder", "Search...");
    $('#print_btn').prop('target', "_blank");
    Metronic.init(); // init metronic core componets
    Layout.init(); // init layout
    Template.init();
});
</script>
{% block js %}{% endblock %}
<!-- END JAVASCRIPTS -->
</body>
<!-- END BODY -->
</html>
