    'bundles/gisttemplate/assets/global/plugins/uniform/jquery.uniform.min.js'
    'bundles/gisttemplate/assets/global/plugins/mustache.min.js'
%}
       <script src="{{ asset_url }}" type="text/javascript"></script>
{% endjavascripts %}
<!-- END CORE PLUGINS -->
