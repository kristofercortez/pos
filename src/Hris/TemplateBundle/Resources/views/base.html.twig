    'bundles/gisttemplate/assets/global/plugins/bootstrap-hover-dropdown/bootstrap-hover-dropdown.min.js'
    'bundles/gisttemplate/assets/global/plugins/jquery-slimscroll/jquery.slimscroll.js'
    'bundles/gisttemplate/assets/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js'
    'bundles/gisttemplate/assets/global/plugins/jquery.blockui.min.js'
