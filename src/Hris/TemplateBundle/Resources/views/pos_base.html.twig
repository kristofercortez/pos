%}
    <link rel="stylesheet" href="{{ asset_url }}" />

{% endstylesheets %}
<!-- END GLOBAL MANDATORY STYLES -->


{% block css %}
<style type="text/css">
.gist_number {
    text-align: right !important;
}

.page-content {
    background-color: #2D2D2D !important;
}
</style>
{% endblock %}
<!-- BEGIN THEME STYLES -->

{% stylesheets
    'bundles/gisttemplate/assets/global/css/components.css'
