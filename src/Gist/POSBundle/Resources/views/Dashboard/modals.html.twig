                            </div>
                            <div class="col-md-6">
                                <h4>Balance <b>Php <span class="co_balance" value="0">0.00</span></b></h4>
                            </div>
                        </div>
                        <div class="row form-horizontal form">
                            <div class="col-md-2">
                                <div style="margin-top: 15px;"></div>
                                <a href="javascript:void(0)" class="co_cash_payment"><img src="{{ asset('bundles/gisttemplate/assets/global/img/pay_cash.png') }}" width="70px" width="70px"></a>
                                <a href="javascript:void(0)" class="co_cc_payment"><img src="{{ asset('bundles/gisttemplate/assets/global/img/pay_card.png') }}" width="70px"></a>
                                <a href="javascript:void(0)" class="co_check_payment"><img src="{{ asset('bundles/gisttemplate/assets/global/img/pay_check.png') }}" width="70px"></a>
                                <a href="javascript:void(0)" class="co_gc_payment"><img src="{{ asset('bundles/gisttemplate/assets/global/img/pay_gc.png') }}" width="70px"></a>
                            </div>
                            <div class="col-md-10">
                                <div style="overflow:scroll; overflow-x: hidden; height: 250px;">
                                    <table class="table table-striped table-bordered table-hover" style="margin-top: 10px;" id="payments_table">
                                        <thead>
                                            <tr>
                                                <th width="40%">Type</th>
                                                <th width="50%">Amount</th>
                                                <th width="10%"></th>
                                            </tr>
                                        </thead>
                                        <tbody id="payments_list">
                                            {% if transaction_object.hasPayments|default(null) %}
                                                {% for p in transaction_object.getPayments %}
                                                    <tr class="row_payment_{{p.getID}}">     
                                                    <input type="hidden" name="control_number" class="control_number" value="{{p.getControlNumber|default('')}}">
                                                    <input type="hidden" name="account_number" class="account_number" value="{{p.getAccountNumber|default('')}}">
                                                    <input type="hidden" name="bank" class="bank" value="{{p.getBank|default('')}}">
                                                    <input type="hidden" name="payment_issued_on" class="payment_issued_on" value="{{p.getPaymentIssuedOn.getID|default('0')}}">
                                                    <input type="hidden" name="cc_terminal_opt" class="cc_terminal_opt" value="{{p.getCardTerminalOperator|default('')}}">
                                                    <input type="hidden" name="cc_interest" class="cc_interest" value="{{p.getInterest|default('')}}">
                                                    <input type="hidden" name="cc_terms" class="cc_terms" value="{{p.getCardTerms|default('')}}">
                                                    <input type="hidden" name="payee" class="payee" value="{{p.getPayee|default('')}}">
                                                    <input type="hidden" name="payor" class="payor" value="{{p.getPayor|default('')}}">
                                                    <input type="hidden" name="cc_expiry" class="cc_expiry" value="{{p.getCardExpiry|default('')}}">
                                                    <input type="hidden" name="cc_cvv" class="cc_cvv" value="{{p.getCardCvv|default('')}}">
                                                    <input type="hidden" name="payment_details_array[]" class="payment_details_array" value="null" >
                                                    <input type="hidden" name="payment_amt_float[]" class="payment_amt_float" value="{{p.amount|default('')}}" >

                                                    <input type="hidden" name="check_type[]" class="check_type" value="{{p.getCheckType|default('')}}" >
                                                    <input type="hidden" name="check_date[]" class="check_date" value="{{p.getCheckDate|default('')}}" >

                                                    <td><input type="text" style="font-size: 12px !important;" name="payment_type[]" value="{{p.getType|default('')}}" readonly="true" class="form-control payment_type"></td>
                                                    <td><input type="text" style="font-size: 10px !important;" name="payment_amt[]" class="form-control payment_amt" readonly="true" value="{{p.amount|number_format(2)|default('')}}"></td>  
                                                    <td ><a href="javascript:void(0)" class="btn btn-xs default red x_remove_payment_row"><i class="fa fa-times" aria-hidden="true"></i></a></td> 
                                                    </tr>
                                                {% endfor %}
                                            {% else %}
                                                <tr class="init_row_payment">
                                                    <td colspan="3"><p>No payments added</p></td>
                                                </tr>
                                            {% endif %}
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <a class="btn green default pull-left quotation_continue_btn switch_to_normal_class" id="switch_to_normal" style="margin=0" href="javascript:void(0)">Switch to Normal Transaction</a>
                        <a class="btn default green finish_btn" style="margin=0" href="javascript:void(0)">Finish</a>
                        <button type="button" data-dismiss="modal" class="btn default" data-toggle="modal">Cancel</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
