<div class="row">
    <div class="col-md-8">
        <!-- BEGIN FORM-->
        <div class="form-body">
             <div class="form-group">
                <label class="col-md-3 control-label"></label>
                <div class="col-md-8">
                    <a href="javascript:void(0)" class="btn blue sync_users">Update Users</a>
                    <br>
                    <table class="table table-striped table-bordered table-hover" style="margin-top: 10px; width: 100%;">
                        <thead>
                            <tr>
                                <th width="10%">ID</th>
                                <th width="10%">Username</th>
                                <th width="30%">Last Name</th>
                                <th width="30%">First Name</th>
                                <th width="10%">Department</th>
                                <th width="10%">Position</th>
                            </tr>
                        </thead>
                        <tbody>
                            {% for u in users %}
                                <tr>
                                    <td><p>{{u.getERPID}}</p></td>
                                    <td><p>{{u.getUsername}}</p></td>
                                    <td><p>{{u.getLastName}}</p></td>
                                    <td><p>{{u.getFirstName}}</p></td>
                                    <td><p>{{u.getDepartment}}</p></td>
                                    <td><p>{{u.getPosition}}</p></td>
                                </tr>
                            {% endfor %}
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label"></label>
                <div class="col-md-4">

                </div>
            </div>
        </div>
    </div>
    <div class="col-md-4">
        <div class="form-body">
        </div>
    </div>
</div>