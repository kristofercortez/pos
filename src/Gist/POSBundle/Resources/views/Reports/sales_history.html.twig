

        <!-- BEGIN FORM-->
        <div class="form-body">
                <div class="row">
                    <div class="col-md-6">
                        {{ form.group_date_range('daterange', 'Date Range', 'date_from', 'date_to', date_from|default(''), date_to|default(''), 4, 7) }}
                    </div>
                    <div class="col-md-6">
                        <button type="submit" class="btn blue">Search</button>
                    </div>                   
                </div>
                <table class="table table-striped table-bordered table-hover" id="list_table">
                    <thead>
                        <tr>
                        {% for col in grid_cols %}
                            {% if (loop.last) %}
                            <th style="width: 145px">{{ col.getLabel }}</th>
                            {% else %}
                            <th>{{ col.getLabel }}</th>
                            {% endif %}
                        {% endfor %}
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
        </div>
        <div class="form-actions fluid">
            <div class="form-group">
                <label class="col-md-3 control-label"></label>
                <div class="col-md-6">
                   
                </div>
            </div>
        </div>


 
